{
  "name": "app-boilerplate",
  "version": "0.0.1",
  "description": "Node.js app boilerplate",
  "main": "index.js",
  "author": "dylanized",
  "license": "MIT",
  "dependencies": {
    "request": "^2.69.0"
  },
  "devDependencies": {
    "mocha": "^2.4.4",
    "unit.js": "^2.0.0"
  },
  "scripts": {
    "==BUILDING==": "",
    "postinstall": "npm run build",
    "postupdate": "bower update && npm run build",
    "build": "",
    "==WATCHING==": "",
    "watch": "npm run watch-css & npm run watch-js & npm run watch-server",
    "watch-css": "",
    "watch-js": "",
    "watch-server": "",
    "==RUNNING==": "",
    "start": "node server.js",
    "build-start": "npm run build && npm run start",
    "watch-start": "npm run watch && npm run start",
    "dev": "npm run reload && npm run build && npm run watch-start",
    "stop": "node server.js",
    "==TESTING==": "",
    "test": "mocha test/[^_]*.js --bail",
    "env": "env",
    "==DATABASE1==": "",
    "createDB": "",
    "importDB": "mongoimport -d $db -c $collection --file ",
    "seedDB": "mongoimport -d $db -c $collection --file $filepath",
    "dropDB": "mongo legislators_development --eval 'db.dropDatabase()'",
    "reloadDB": "npm run dropDB && npm run seedDB",
    "exportDB": "",
    "startDB": "",
    "==DATABASE2==": "",
    "createSQL": "",
    "importSQL": "",
    "seedSQL": "",
    "dropSQL": "",
    "reloadSQL": "",
    "exportSQL": "",
    "startSQL": "",
    "==OTHER==": "",
    "cron": "",
    "cleanup": ""
  }
}
